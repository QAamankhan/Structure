<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
<groups>
<run>
<include name="productResult"></include>
</run>
</groups>
<listeners>
<listener class-name="utilities.ListenerClass"></listener>
</listeners>

  <test thread-count="5" name="SignIn">
    <classes>
      <class name="testCases.SignInValidation"/>
    </classes>
  </test> <!-- Test -->
  
   <test thread-count="5" name="HomePage">
    <classes>
      <class name="testCases.HomePageValidation"></class>
    </classes>
  </test> <!-- Test -->
  
  
   <test thread-count="5" name="ProductPage">
    <classes>
      <class name="testCases.ProductResultPageValidation"></class>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
